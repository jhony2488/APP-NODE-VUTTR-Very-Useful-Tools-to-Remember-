{
  "name": "api-node-vuttr-very-useful-tools-to-remember",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "start": "nodemon dist/server.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all ",
    "pretest-stress": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize-cli db:seed:all",
    "test-stress": "NODE_ENV=test jest --stressRuns=24 --detectOpenHandles",
    "posttest-stress": "NODE_ENV=test sequelize-cli db:seed:undo && NODE_ENV=test sequelize db:migrate:undo:all  ",
    "predev": "NODE_ENV=development sequelize db:migrate",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify ./src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "run-migrations-test": "NODE_ENV=test sequelize db:migrate",
    "run-migrations-dev": "NODE_ENV=development sequelize db:migrate",
    "run-migrations": "sequelize db:migrate",
    "undo-migrations-test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "undo-migrations-dev": "NODE_ENV=development sequelize db:migrate:undo:all",
    "undo-migrations": "sequelize db:migrate:undo:all"
  },
  "author": "Jhonata Vinicius",
  "license": " GNU GENERAL PUBLIC LICENSE",
  "dependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.12.7",
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.0.0",
    "slugify": "^1.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "factory-girl": "^5.0.4",
    "faker": "^5.5.2",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
